    "@mse/sample-cards": "workspace:*",
    "@mse/symbols": "workspace:*",
    "@mse/fonts.beleren": "workspace:*",
    "@mse/fonts.mplantin": "workspace:*",
    "@mse/templates.m15": "workspace:*",
