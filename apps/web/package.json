{
  "name": "@mse/web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "pnpm copy-assets && next dev -p 4000",
    "build:web": "pnpm copy-assets && next build",
    "start": "pnpm copy-assets && next start",
    "lint": "TIMING=1 next lint",
    "format": "prettier --write .",
    "copy-assets": "cp -r -L ${PWD}/node_modules/@mse/assets/dist/* ${PWD}/public || true",
    "link-assets": "ln -s ${PWD}/node_modules/@mse/assets/dist/* ${PWD}/public || true"
  },
  "dependencies": {
    "@emotion/react": "11.10.0",
    "@emotion/styled": "11.10.0",
    "@mse/ui.card": "workspace:*",
    "@mse/ui.core": "workspace:*",
    "@mse/utils.card": "workspace:*",
    "@mui/icons-material": "5.8.4",
    "@mse/assets": "workspace:*",
    "@mui/material": "5.10.1",
    "@mse/sample-cards": "workspace:*",
    "next": "12.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@mse/eslint-config": "workspace:*",
    "@mse/tsconfig": "workspace:*",
    "@mse/types": "workspace:*",
    "eslint": "8.13.0",
    "@babel/core": "^7.18.13",
    "next-transpile-modules": "9.0.0",
    "typescript": "^4.7.4"
  },
  "resolutions": {
    "react": "18.0.17"
  }
}
